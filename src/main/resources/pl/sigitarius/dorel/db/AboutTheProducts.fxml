<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="pl.sigitarius.dorel.db.AboutTheProductsController">
    <center>
        <TableView fx:id="atpTable" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <columns>
                <TableColumn fx:id="articleNumberCol" prefWidth="75.0" text="article_number"/>
                <TableColumn fx:id="eanCol" prefWidth="75.0" text="ean"/>
                <TableColumn fx:id="introTextCol" prefWidth="75.0" text="intro_text_consumer"/>
                <TableColumn fx:id="longDescCol" prefWidth="75.0" text="long_description_2nd_part"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </center>
</BorderPane>
