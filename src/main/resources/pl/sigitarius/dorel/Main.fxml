<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.sigitarius.dorel.MainController">
   <center>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="169.0" prefWidth="400.0" />
   </center>
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Narzędzia">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#defaultConnection" text="Połączenie DB" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="Akcje">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#load" text="Wczytaj XML" />
                  <MenuItem mnemonicParsing="false" onAction="#downloadImages" text="Ściągnij obrazki na dysk" />
               </items>
            </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
